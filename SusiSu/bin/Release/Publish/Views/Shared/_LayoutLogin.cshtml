<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @*@Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")*@
    <link rel="stylesheet" href="~/Content/LoginFolder/fonts/material-icon/css/material-design-iconic-font.min.css">
    <link rel="stylesheet" href="~/Content/LoginFolder/css/style.css">
    <link href="https://fonts.googleapis.com/css?family=Exo+2:400,500,600,700%7CSchoolbell" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/assets/css/style.css">
</head>
<body style="background-image: url('../../Content/assets/img/water.png')">
    <div class="preLoader">
        <div class="preload-img">
            <img src='~/Content/assets/img/preloader.gif' alt="" class="pre-img">
        </div>
    </div>
    <div>
        @RenderBody()
    </div>
    <!-- JS      bu kısmıda layouttan aldık kesip yapıstırdık-->
    <script src="~/Content/LoginFolder/vendor/jquery/jquery.min.js"></script>
    <script src="~/Content/LoginFolder/js/main.js"></script>

    <script src="~/Content/assets/js/jquery-3.3.1.min.js"></script>
    <script src="~/Content/assets/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/assets/plugins/waypoints/jquery.waypoints.min.js"></script>
    <script src="~/Content/assets/plugins/parsley/parsley.min.js"></script>
    <script src="~/Content/assets/plugins/retinajs/retina.min.js"></script>
    <script src="~/Content/assets/plugins/parallax/parallax.js"></script>
    <script src="~/Content/assets/plugins/owl-carousel/owl.carousel.min.js"></script>
    <script src="~/Content/assets/js/menu.min.js"></script>
    <script src="~/Content/assets/plugins/Magnific-Popup/jquery.magnific-popup.min.js"></script>
    <script src="~/Content/assets/plugins/waypoints/jquery.counterup.min.js"></script>
    <script src="~/Content/assets/plugins/camera/camera.min.js"></script>
    <script src="~/Content/assets/js/easing.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB2D8wrWMY3XZnuHO6C31uq90JiuaFzGws"></script>
    <script src="~/Content/assets/js/scripts.js"></script>
    <script src="~/Content/assets/js/custom.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>

</html>
